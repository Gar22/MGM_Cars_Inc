<article>
      <header>
        <h2><%= car.year %><%= car.make %><%= car.model %>
      </header>
    <p>
    <table>
      <tr>
        <th> Color:</th>
        <td> <%= car.color %><br></td>
      </tr>
      <tr>
        <th>Mileage:</th>
        <td><%= car.mileage %></td>
        </tr>
      <tr>
        <th> Vehicle type: </th>
        <td><%= car.type %></td>
      </tr>
      <tr>
        <th>Price: </th>
        <td><%= car.sale_price %></td>
      </tr>
    </table>
  </article>

  
<nav>
  <%= link_to "All Cars", cars_path %> |
  <%= link_to "Edit", edit_car_path(@car) %>
</nav>